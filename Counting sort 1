  public static List<Integer> countingSort(List<Integer> arr) {
    // Write your code here
 int [] li = new int [100];
 List <Integer> n= new ArrayList<>();
    for(int i=0; i< 100; i++){
        int pl= arr.get(i);
     li[pl]++;
    }
    for(int i =0; i<100 ; i++){
        n.add(li[i]);
        
    }
return n; 
    }
