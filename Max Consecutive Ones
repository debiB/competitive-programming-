class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
       int i, max=0,count =0;
        for(i=0; i < nums.length; i++){
            
        if(nums[i] ==1){
            count++;
            max =Math.max(max,count);
        }else {
            count =0; 
        }   
    }
        return max;
        }
}
